{
  "name": "redux-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.31",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.5.0",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "scripts": {
    "build-css":
      "node-sass-chokidar --include-path ./src src/components -o src/components",
    "watch-css":
      "npm run build-css && node-sass-chokidar --include-path ./src src/components/ -o src/components --watch --recursive",
    "postinstall": "npm run build",
    "debug": "concurrently \"react-scripts start\" \"npm run watch-css\"",
    "build": "react-scripts build && npm run build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "node ./bin/www"
  }
}
